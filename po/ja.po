#. type: Plain text
#: zenity.1:4
msgid "zenity - display GTK+ dialogs"
msgstr "zenity - GTK+ のダイアログを表示する"

#. type: SH
#: zenity.1:4
#, no-wrap
msgid "SYNOPSIS"
msgstr "書式"

#. type: Plain text
#: zenity.1:7
msgid "B<zenity> [I<options>]"
