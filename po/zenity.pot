msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: zenity.1:345
msgid ""
"Display a file selector with the title I<Select a file to remove>. The file "
"selected is returned on standard output."
msgstr ""

#. type: Plain text
#: zenity.1:347
msgid "zenity --title=\"Select a file to remove\" --file-selection"
msgstr ""

#. type: Plain text
#: zenity.1:351
msgid ""
"Display a text entry dialog with the title I<Select Host> and the text "
"I<Select the host you would like to flood-ping>. The entered text is "
"returned on standard output."
msgstr ""

#. type: Plain text
#: zenity.1:353
msgid ""
"zenity --title \"Select Host\" --entry --text \"Select the host you would "
"like to flood-ping\""
msgstr ""

#. type: Plain text
#: zenity.1:357
msgid ""
"Display a dialog, asking I<Microsoft Windows has been found! Would you like "
"to remove it?>. The return code will be 0 (true in shell)  if I<OK> is "
"selected, and 1 (false) if I<Cancel> is selected."
msgstr ""

#. type: Plain text
#: zenity.1:359
msgid ""
"zenity --question --title \"Alert\" --text \"Microsoft Windows has been "
"found! Would you like to remove it?\""
msgstr ""

#. type: Plain text
#: zenity.1:362
msgid ""
"Show the search results in a list dialog with the title I<Search Results> "
"and the text I<Finding all header files...>."
msgstr ""

#. type: Plain text
#: zenity.1:364
msgid ""
"find . -name '*.h' | zenity --list --title \"Search Results\" --text "
"\"Finding all header files..\" --column \"Files\""
msgstr ""

#. type: Plain text
#: zenity.1:366
msgid "Show a notification in the message tray"
msgstr ""

#. type: Plain text
#: zenity.1:368
msgid ""
"zenity --notification --window-icon=update.png --text \"System update "
"necessary!\""
msgstr ""

#. type: Plain text
#: zenity.1:370
msgid ""
"Display a weekly shopping list in a check list dialog with I<Apples> and "
"I<Oranges> pre selected"
msgstr ""

#. type: Plain text
#: zenity.1:372
msgid ""
"zenity --list --checklist --column \"Buy\" --column \"Item\" TRUE Apples "
"TRUE Oranges FALSE Pears FALSE Toothpaste"
msgstr ""

#. type: Plain text
#: zenity.1:374
msgid ""
"Display a progress dialog while searching for all the postscript files in "
"your home directory"
msgstr ""

#. type: Plain text
